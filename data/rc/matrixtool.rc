<!DOCTYPE kpartgui SYSTEM "kpartgui.dtd">

<kpartgui name="Rosegarden::MatrixTool" version="1230">

<Menu name="MatrixTool">
  <Action name="multitool" text="Switch to Multi Tool" icon="multitool"
   shortcut="F2"/>
  <Action name="draw" text="Switch to Draw Tool" icon="pencil" shortcut="F3"/>
  <Action name="erase" text="Switch to Erase Tool" icon="eraser" shortcut="F4"/>
  <Action name="move" text="Switch to Move Tool" icon="move" shortcut="F5"/>
  <Action name="resize" text="Switch to Resize Tool" icon="resize"
   shortcut="F6"/>
  <Action name="velocity" text="Switch to Velocity Tool" icon="velocity"
   shortcut="F7"/> 
  <Separator/>
  <Action name="undo" text="Undo" icon="undo"/>
  <Action name="redo" text="Redo" icon="redo"/>
  <Separator/>
</Menu>

</kpartgui>
